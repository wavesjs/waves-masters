{
  "name": "waves-masters",
  "version": "2.0.0",
  "description": "low level components for transport and scheduling",
  "authors": [
    "Norbert Schnell",
    "Benjamin Matuszewski",
    "Victor Saiz",
    "Karim Barkati",
    "Samuel Goldszmidt"
  ],
  "main": "dist/index.js",
  "license": "BSD-3-Clause",
  "repository": {
    "type": "git",
    "url": "https://github.com/wavesjs/waves-masters"
  },
  "bugs": {
    "url": "https://github.com/wavesjs/waves-masters/issues"
  },
  "homepage": "http://wavesjs.github.io/waves-masters",
  "keywords": [
    "dsp",
    "web audio",
    "scheduling",
    "audio player",
    "audio transport"
  ],
  "scripts": {
    "clean": "rm -Rf dist",
    "deploy": "np --yolo",
    "doc": "jsdoc -c jsdoc.json",
    "prepublishOnly": "npm run transpile",
    "version": "npm run clean && npm run transpile",
    "transpile": "babel src --out-dir dist",
    "watch": "npm run clean && npm run transpile && chokidar src -c \"npm run transpile\""
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/plugin-transform-modules-commonjs": "^7.4.4",
    "chokidar": "^3.0.1",
    "chokidar-cli": "^1.2.2",
    "concurrently": "^4.1.0",
    "np": "^6.1.0"
  }
}

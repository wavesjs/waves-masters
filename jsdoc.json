{
  "source": {
    "include": [
      "README.md",
      "src",
      "src/core",
      "src/engines",
      "src/masters"
    ],
    "includePattern": ".+\\.js(doc)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "opts": {
    "destination": "./docs",
    "encoding": "utf8",
    "template": "node_modules/@ircam/jsdoc-template"
  },
  "templates": {
    "default": {
      "useLongnameInNav": true
    }
  }
}
